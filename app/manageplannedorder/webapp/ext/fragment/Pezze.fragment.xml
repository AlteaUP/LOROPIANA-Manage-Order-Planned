<core:FragmentDefinition
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    xmlns:form="sap.ui.layout.form"
>
    <Dialog
        id="_IDGenDialogPezze"
        stretch="true"
        resizable="false"
        type="Message"
        state="Information"
        icon="sap-icon://ai"
        title="Assegna Pezze"
    >
        <content>
            <Text
                id="_IDGenTextPezze"
                text="Assegna Pezze"
            />
            <form:SimpleForm
                id="SimpleFormPezze"
                columnsL="3"
                columnsM="3"
                columnsXL="3"
                emptySpanL="0"
                emptySpanM="0"
                emptySpanXL="0"
                labelSpanL="12"
                labelSpanM="12"
                labelSpanXL="12"
                visible="true"
                editable="false"
                singleContainerFullSize="false"
                layout="ColumnLayout"
            >
                <Label
                    text="AvailableQuantity"
                    id="labelAvailableQuantity"
                />
                <Input
                    value="{/AvailableQuantity}"
                    enabled="false"
                    id="inputAvailableQuantity"
                />
                <Label
                    text="BOMItem"
                    id="labelBOMItem"
                />
                <Input
                    value="{/BOMItem}"
                    enabled="false"
                    id="inputBOMItem"
                />
                <Label
                    text="BOMItemDescription"
                    id="labelBOMItemDescription"
                />
                <Input
                    value="{/BOMItemDescription}"
                    enabled="false"
                    id="inputBOMItemDescription"
                />
                <Label
                    text="BaseUnit"
                    id="labelBaseUnit"
                />
                <Input
                    value="{/BaseUnit}"
                    enabled="false"
                    id="inputBaseUnit"
                />
                <Label
                    text="BillOfMaterialCategory"
                    id="labelBillOfMaterialCategory"
                />
                <Input
                    value="{/BillOfMaterialCategory}"
                    enabled="false"
                    id="inputBillOfMaterialCategory"
                />
                <Label
                    text="CplndOrd"
                    id="labelCplndOrd"
                />
                <Input
                    value="{/CplndOrd}"
                    enabled="false"
                    id="inputCplndOrd"
                />
                <Label
                    text="MRPController"
                    id="labelMRPController"
                />
                <Input
                    value="{/MRPController}"
                    enabled="false"
                    id="inputMRPController"
                />
                <Label
                    text="MRPControllerName"
                    id="labelMRPControllerName"
                />
                <Input
                    value="{/MRPControllerName}"
                    enabled="false"
                    id="inputMRPControllerName"
                />
                <Label
                    text="Material"
                    id="labelMaterial"
                />
                <Input
                    value="{/Material}"
                    enabled="false"
                    id="inputMaterial"
                />
                <Label
                    text="MatlCompRequirementDate"
                    id="labelMatlCompRequirementDate"
                />
                <Input
                    value="{/MatlCompRequirementDate}"
                    enabled="false"
                    id="inputMatlCompRequirementDate"
                />
                <Label
                    text="Plant"
                    id="labelPlant"
                />
                <Input
                    value="{/Plant}"
                    enabled="false"
                    id="inputPlant"
                />
                <Label
                    text="PlantName"
                    id="labelPlantName"
                />
                <Input
                    value="{/PlantName}"
                    enabled="false"
                    id="inputPlantName"
                />
                <Label
                    text="ProductCharacteristic1"
                    id="labelProductCharacteristic1"
                />
                <Input
                    value="{/ProductCharacteristic1}"
                    enabled="false"
                    id="inputProductCharacteristic1"
                />
                <Label
                    text="ProductCharacteristic2"
                    id="labelProductCharacteristic2"
                />
                <Input
                    value="{/ProductCharacteristic2}"
                    enabled="false"
                    id="inputProductCharacteristic2"
                />
                <Label
                    text="ProductCharacteristic3"
                    id="labelProductCharacteristic3"
                />
                <Input
                    value="{/ProductCharacteristic3}"
                    enabled="false"
                    id="inputProductCharacteristic3"
                />
                <Label
                    text="RequiredQuantity"
                    id="labelRequiredQuantity"
                />
                <Input
                    value="{/RequiredQuantity}"
                    enabled="false"
                    id="inputRequiredQuantity"
                />
                <Label
                    text="StorageLocation"
                    id="labelStorageLocation"
                />
                <Input
                    value="{/StorageLocation}"
                    enabled="false"
                    id="inputStorageLocation"
                />
                <Label
                    text="TotalAllocQty"
                    id="labelTotalAllocQty"
                />
                <Input
                    value="{/TotalAllocQty}"
                    enabled="false"
                    id="inputTotalAllocQty"
                />
                <Label
                    text="WithdrawnQuantity"
                    id="labelWithdrawnQuantity"
                />
                <Input
                    value="{/WithdrawnQuantity}"
                    enabled="false"
                    id="inputWithdrawnQuantity"
                />
            </form:SimpleForm>

            <Table
                items="{/selectedItems}"
                id="selectedItemsTable"
            >
                <columns>
                    <Column id="columnBatch">
                        <Text
                            id="textBatch"
                            text="Batch"
                        />
                    </Column>
                    <Column id="columnMaterial">
                        <Text
                            id="textMaterial"
                            text="Material"
                        />
                    </Column>
                    <Column id="columnStorageLocation">
                        <Text
                            id="textStorageLocation"
                            text="StorageLocation"
                        />
                    </Column>
                    <Column id="columnMatlCnsmpnQtyInMatlBaseUnit">
                        <Text
                            id="textMatlCnsmpnQtyInMatlBaseUnit"
                            text="MatlCnsmpnQtyInMatlBaseUnit"
                        />
                    </Column>
                    <Column id="columnMatlStkDecrQtyInMatlBaseUnit">
                        <Text
                            id="textMatlStkDecrQtyInMatlBaseUnit"
                            text="MatlStkDecrQtyInMatlBaseUnit"
                        />
                    </Column>
                    <Column id="columnMatlStkIncrQtyInMatlBaseUnit">
                        <Text
                            id="textMatlStkIncrQtyInMatlBaseUnit"
                            text="MatlStkIncrQtyInMatlBaseUnit"
                        />
                    </Column>
                    <Column id="columnMatlWrhsStkQtyInMatlBaseUnit">
                        <Text
                            id="textMatlWrhsStkQtyInMatlBaseUnit"
                            text="MatlWrhsStkQtyInMatlBaseUnit"
                        />
                    </Column>
                </columns>
                <items>
                    <ColumnListItem id="selectedItemsTableItem">
                        <cells>
                            <ObjectIdentifier
                                id="valueBatch"
                                title="{Batch}"
                            />
                            <Text
                                id="valueMaterial"
                                text="{Material}"
                            />
                            <Text
                                id="valueStorageLocation"
                                text="{StorageLocation}"
                            />
                            <Input
                                type="Number"
                                id="valueMatlCnsmpnQtyInMatlBaseUnit"
                                value="{MatlCnsmpnQtyInMatlBaseUnit}"
                            />
                            <Input
                                type="Number"
                                id="valueMatlStkDecrQtyInMatlBaseUnit"
                                value="{MatlStkDecrQtyInMatlBaseUnit}"
                            />
                            <Input
                                type="Number"
                                id="valueMatlStkIncrQtyInMatlBaseUnit"
                                value="{MatlStkIncrQtyInMatlBaseUnit}"
                            />
                            <Input
                                type="Number"
                                id="valueMatlWrhsStkQtyInMatlBaseUnit"
                                value="{MatlWrhsStkQtyInMatlBaseUnit}"
                            />
                        </cells>
                    </ColumnListItem>
                </items>
            </Table>
        </content>
        <buttons>
            <Button
                id="_IDGenBtnAssegnaPezze"
                text="Assegna"
                type="Accept"
                press=".doAssign"
            />
            <!-- core:require="{ handler: 'manageplannedorder/manageplannedorder/ext/controller/Pezze'}" -->
            <Button
                id="_IDGenBtnCancelPezze"
                text="Chiudi"
                type="Back"
                press=".onCloseDialog"
            />
            <!-- core:require="{ handler: 'manageplannedorder/manageplannedorder/ext/controller/Pezze'}" -->
        </buttons>
    </Dialog>
</core:FragmentDefinition>
