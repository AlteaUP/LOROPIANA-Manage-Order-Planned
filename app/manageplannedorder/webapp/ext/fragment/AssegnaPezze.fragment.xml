<core:FragmentDefinition
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    xmlns:form="sap.ui.layout.form"
    xmlns:macros="sap.fe.macros"
>
    <Dialog
        id="_IDGenDialog1234"
        stretch="true"
        resizable="false"
        type="Message"
        state="Information"
        icon="sap-icon://ai"
        title="Assegna Pezze {CplndOrd}"
    >
        <content>
            <form:SimpleForm
                id="_IDGenSimpleForm1234"
                editable="true"
                layout="ColumnLayout"
                labelSpanL="12"
                labelSpanM="12"
                labelSpanS="12"
                emptySpanL="0"
                emptySpanM="0"
                emptySpanS="0"
                columnsL="2"
                columnsM="2"
                columnsXL="2"
            >
                <Label
                    id="_formCplndOrd"
                    text="CplndOrd"
                />
                <Input
                    id="_IDGenInputCplndOrd"
                    value="{CplndOrd}"
                    enabled="false"
                />

                <Label
                    id="_formMaterial"
                    text="Material"
                />
                <Input
                    id="_IDGenInputMaterial"
                    value="{Material}"
                    enabled="false"
                />

                <Label
                    id="_formPlant"
                    text="Plant"
                />
                <Input
                    id="_IDGenInputPlant"
                    value="{Plant}"
                    enabled="false"
                />
            </form:SimpleForm>
            <!-- Stock Table -->
            <macros:Table
                id="TableStock"
                metaPath="/ZZ1_CombinPlannedOrdersCom/@com.sap.vocabularies.UI.v1.LineItem#stock"
                header="Stocks"
            />
        </content>
        <buttons>
            <Button
                id="_IDGenBtnCancel"
                text="Annulla"
                type="Reject"
                press="onCancel"
            />
        </buttons>
    </Dialog>
</core:FragmentDefinition>
