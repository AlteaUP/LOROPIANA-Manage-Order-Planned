<core:FragmentDefinition
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    xmlns:form="sap.ui.layout.form"
>
    <Dialog
        id="_IDGenDialogChangeWorkCenter"
        stretch="true"
        resizable="false"
        type="Message"
        state="Information"
        icon="sap-icon://ai"
        title="Change Work Center {CplndOrd}"
    >
        <content />
        <buttons>
            <Button
                id="_IDGenBtnCancelWork"
                text="Annulla"
                type="Reject"
                press="onCancel"
            />
        </buttons>
    </Dialog>
</core:FragmentDefinition>
